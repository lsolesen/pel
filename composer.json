    "keywords": [
        "image", "exif"
    ],
    "homepage": "http://pel.github.com/pel/",
    "authors": [
        {
            "name": "Lars Olesen",
            "email": "lars@intraface.dk",
            "homepage": "http://intraface.dk",
            "role": "Developer"
        },
        {
            "name": "Martin Geisler",
            "email": "martin@geisler.net",
            "homepage": "http://geisler.net",
            "role": "Developer"
        }
    ],
    "license": "GPL-2.0",
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev" : {
        "ext-gd": "*",
        "phpunit/phpunit": ">=7 <8",
        "squizlabs/php_codesniffer": ">3.5",
        "satooshi/php-coveralls": ">2.4"
    },
    "autoload": {
        "psr-4": {"lsolesen\\pel\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": { "Pel\\Test\\": "test/" }
    }
